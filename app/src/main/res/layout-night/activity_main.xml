<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.motion.widget.MotionLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#040303"
    android:backgroundTint="#2B2A2A"
    app:layoutDescription="@xml/activity_main_scene4"
    tools:context=".MainActivity">

    <CheckBox
        android:id="@+id/checkBox3"
        android:layout_width="211dp"
        android:layout_height="41dp"
        android:button="@android:drawable/radiobutton_off_background"
        android:buttonTint="#FF5722"
        android:fontFamily="@font/caladea"
        android:text="Not Accurate"
        android:textColor="#FF5722"
        android:textSize="20"
        android:textStyle="italic"
        android:typeface="normal"
        tools:layout_editor_absoluteX="149dp"
        tools:layout_editor_absoluteY="521dp" />

    <TextView
        android:id="@+id/lineReview"
        android:layout_width="303dp"
        android:layout_height="38dp"
        android:text="Were the lines on time today?"
        android:textAlignment="center"
        android:textColor="#FDFAFA"
        android:textSize="20"
        tools:layout_editor_absoluteX="54dp"
        tools:layout_editor_absoluteY="98dp" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="297dp"
        android:layout_height="64dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Thank you for your feedback!"
        android:textAlignment="center"
        android:textColor="#F6F3F3"
        tools:layout_editor_absoluteX="54dp"
        tools:layout_editor_absoluteY="562dp" />

    <CheckBox
        android:id="@+id/checkBox1"
        style="@android:style/Widget.Holo.CompoundButton.CheckBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:button="@android:drawable/radiobutton_off_background"
        android:buttonTint="#4EC34A"
        android:clickable="true"
        android:fontFamily="@font/caladea"
        android:text="Amazing!"
        android:textColor="#24B324"
        android:textSize="20"
        android:textStyle="italic"
        tools:layout_editor_absoluteX="149dp"
        tools:layout_editor_absoluteY="434dp" />

    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="45dp"
        android:layout_height="39dp"
        android:button="@android:drawable/radiobutton_off_background"
        android:buttonTint="#FF9800"
        android:fontFamily="@font/caladea"
        android:text="Ok"
        android:textColor="#FF9800"
        android:textSize="20"
        android:textStyle="italic"
        tools:layout_editor_absoluteX="149dp"
        tools:layout_editor_absoluteY="482dp" />

    <EditText
        android:id="@+id/EstimateReview"
        android:layout_width="302dp"
        android:layout_height="64dp"
        android:ems="10"
        android:fontFamily="sans-serif"
        android:inputType="textPersonName"
        android:text="How were our estimates?"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#FBF9F9"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="370dp" />

    <ImageButton
        android:id="@+id/threeLines"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#FFFCFC"
        android:tint="#101010"
        app:srcCompat="@android:drawable/ic_menu_sort_by_size"
        tools:layout_editor_absoluteX="2dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="61dp"
        android:layout_height="56dp"
        android:backgroundTint="#939090"
        app:srcCompat="@android:drawable/btn_radio"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="170dp" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="61dp"
        android:layout_height="56dp"
        android:backgroundTint="#939090"
        app:srcCompat="@android:drawable/btn_radio"
        tools:layout_editor_absoluteX="54dp"
        tools:layout_editor_absoluteY="235dp" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="61dp"
        android:layout_height="56dp"
        android:backgroundTint="#939090"
        app:srcCompat="@android:drawable/btn_radio"
        tools:layout_editor_absoluteX="54dp"
        tools:layout_editor_absoluteY="299dp" />

    <EditText
        android:id="@+id/line1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="[insert line name]"
        android:textColor="#FFFCFC"
        tools:layout_editor_absoluteX="115dp"
        tools:layout_editor_absoluteY="170dp" />

    <EditText
        android:id="@+id/line"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="[insert line name]"
        android:textColor="#F6F4F4"
        tools:layout_editor_absoluteX="119dp"
        tools:layout_editor_absoluteY="246dp" />

    <EditText
        android:id="@+id/line2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="[insert line name]"
        android:textColor="#FAF7F7"
        tools:layout_editor_absoluteX="116dp"
        tools:layout_editor_absoluteY="310dp" />

    <TextView
        android:id="@+id/LOGO"
        android:layout_width="294dp"
        android:layout_height="68dp"
        android:fontFamily="@font/alfa_slab_one"
        android:text="TripTap"
        android:textAlignment="center"
        android:textColor="#F8F4F4"
        android:textSize="40"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="58dp"
        tools:layout_editor_absoluteY="2dp" />

    <Button
        android:id="@+id/mainmenu"
        style="@style/Widget.AppCompat.Button.Borderless.Colored"
        android:layout_width="251dp"
        android:layout_height="55dp"
        android:background="#F60B0B"
        android:backgroundTint="#F61010"
        android:text="Return to Main Menu"
        app:cornerRadius="10"
        app:iconTint="#ED0606"
        app:rippleColor="#ED0909"
        app:strokeColor="#EA0404"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="647dp" />

    <ProgressBar
        android:id="@+id/progressBar"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="1dp" />

    <androidx.constraintlayout.helper.widget.Layer
        android:id="@+id/layer3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#2B2A2A"
        android:backgroundTint="#2B2A2A" />

</androidx.constraintlayout.motion.widget.MotionLayout>